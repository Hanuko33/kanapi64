#cvs
cvs -d:pserver:anonymous@flex.cvs.sourceforge.net:/cvsroot/flex login; cvs -z3 -d:pserver:anonymous@flex.cvs.sourceforge.net:/cvsroot/flex co -P flex
cvs -d:pserver:anonymous@expat.cvs.sourceforge.net:/cvsroot/expat login; cvs -z3 -d:pserver:anonymous@expat.cvs.sourceforge.net:/cvsroot/expat co expat
cvs -z3 -d:pserver:anonymous@cvs.savannah.gnu.org:/web/ncurses co ncurses
cvs -d :pserver:anonymous@rpm5.org:/cvs login; cvs -d :pserver:anonymous@rpm5.org:/cvs co rpm
cvs -z 9 -d :pserver:anoncvs@sourceware.org:/cvs/src login ;  cvs -z 9 -d :pserver:anoncvs@sourceware.org:/cvs/src co binutils
cvs -d anonymous@cvs.openssl.org:/openssl-cvs co openssl
cvs -d:pserver:anonymous@cvs.sv.gnu.org:/sources/make co make 
cvs -d:pserver:anonymous@procps.cvs.sourceforge.net:/cvsroot/procps login; cvs -z3 -d:pserver:anonymous@procps.cvs.sourceforge.net:/cvsroot/procps co -P procps

mv src binutils
